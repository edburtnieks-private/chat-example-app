{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "react/jsx-filename-extension": [
      1,
      {
        "extensions": [".js"]
      }
    ],
    "import/no-extraneous-dependencies": [
      "error",
      {
        "devDependencies": true
      }
    ],
    "linebreak-style": "off",
    "global-require": "off",
    "arrow-parens": "off",
    "comma-dangle": "off",
    "new-cap": "off"
  },
  "globals": {
    "window": true,
    "document": true
  }
}
